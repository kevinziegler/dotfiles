\set QUIET 1

\pset null '¤'

\set PROMPT1 '%[%033[1;35m%]%n@%/ %[%033[1;32m%]%m %[%033[1;31m%]\n❯❯❯ %[%033[0m%]%'
\set PROMPT2 '%R ... > '

\timing

\x auto
\set ENCODING utf-8
\set VERBOSITY verbose
\set HISTFILE ~/.psql_history- :DBNAME
\set HISTCONTROL ignoredups
\set COMP_KEYWORD_CASE upper

\pset linestyle unicode
\pset border 2
\unset QUIET
